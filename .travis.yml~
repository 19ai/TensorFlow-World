language: python
python:

  - "3.5"

# command to install dependencies
install:
  - pip install -r requirements.txt
  - pip install coveralls

script:

  - coverage run --omit=*.virtualenvs*,*virtualenv* codes/3-neural_networks/convolutional-neural-network/code/train_classifier.py test



after_success:
  coveralls

sudo: enabled
dist: trusty
